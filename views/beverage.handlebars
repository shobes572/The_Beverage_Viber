<!-- main page content -->
  <main>
    <!-- parallax -->
    <div class="parallax-container">
      <div class="parallax"><img src="images/erol-ahmed-54hUU5pNSvo-unsplash.jpeg"></div>
    </div>
    <div class="section white">
      <!-- beverage card -->
      <div class="row container">
        <div class="row beverage-row">
          <div class="col s12">
          <!-- TESTING controlling card size -->
            <div class="card col s12">
              <div class="card-image">
                <img src= {{image}}>
                <span class="card-title">{{beverage}}</span>
              </div>
              <div class="card-content">
                <p>{{info}}</p>
              </div>
              <div class="card-action">
              </div>
          </div>
        </div>
      </div>

    </div>      
    <!-- REMOVED second parallax to make bottom of page less crowded -->
    
  </main>